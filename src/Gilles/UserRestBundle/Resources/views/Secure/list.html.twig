{# src/Gilles/UserRestBundle/Ressources/views/secure/login.html.twig #}

{% block body %}
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>First Name</th>
            <th>Mail</th>
        </tr>
    </thead>
    <tbody>
{% for user in users %}
    <tr>
        <td>           
            {{ user.name }}
        </td>
        <td>           
            {{ user.firstname }}
        </td>
        <td>           
            {{ user.mail }}
        </td>
    </tr>
{% endfor %}
    </tbody>
</table>
{% endblock %}